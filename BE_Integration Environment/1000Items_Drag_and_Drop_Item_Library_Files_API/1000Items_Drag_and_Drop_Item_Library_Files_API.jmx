import groovy.json.JsonSlurper

def baseSectionNumber = (props.get(&quot;sectionNumber&quot;) ?: &quot;2000&quot;) as Integer
int threadNum = ctx.getThreadNum() + 1
def rawSectionNumber = baseSectionNumber + threadNum * 1000

log.info(&quot; Generating section number with DnD prefix for thread ${threadNum}...&quot;)

def isSectionNumberExists = { String number -&gt;
    def checkUrl = &quot;https://your-api.com/check-section?number=${number}&quot;
    def conn = new URL(checkUrl).openConnection()
    conn.setRequestMethod(&quot;GET&quot;)
    conn.setRequestProperty(&quot;Accept&quot;, &quot;application/json&quot;)
    try {
        def responseText = conn.getInputStream().getText(&quot;UTF-8&quot;)
        def json = new JsonSlurper().parseText(responseText)
        def exists = json.exists == true
        log.info(&quot; Section &apos;${number}&apos; exists? ${exists}&quot;)
        return exists
    } catch (Exception e) {
        log.warn(&quot; Check failed for section &apos;${number}&apos;: ${e.message}&quot;)
        return false
    }
}

def numericSection = rawSectionNumber
def sectionNumber = &quot;DnD${numericSection}&quot;
def suffix = 1
while (isSectionNumberExists(sectionNumber)) {
    numericSection = rawSectionNumber + suffix
    sectionNumber = &quot;DnD${numericSection}&quot;
    suffix++
    log.info(&quot;️ Section number already exists. Trying: ${sectionNumber}&quot;)
}

vars.put(&quot;projectSectionNumber&quot;, sectionNumber)
vars.put(&quot;aggrDesc&quot;, &quot;Section Description on ${Section description} for ${sectionNumber}&quot;)

if (threadNum == 1) {
    props.put(&quot;sectionNumber&quot;, (baseSectionNumber + 1).toString())
    log.info(&quot; Incremented baseSectionNumber to: ${baseSectionNumber + 1}&quot;)
}

log.info(&quot; Final Section Number: ${sectionNumber}&quot;)
