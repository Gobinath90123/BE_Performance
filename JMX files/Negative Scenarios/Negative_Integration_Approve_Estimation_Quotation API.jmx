def portfolioTitle = vars.get(&quot;PORTFOLIO_TITLE&quot;) ?: &quot;PORTFOLIO_TITLE_NOT_SET&quot;

log.info(&quot;============================================================&quot;)
log.info(&quot;📁 Extracting portfolioId for title: ${portfolioTitle}&quot;)
