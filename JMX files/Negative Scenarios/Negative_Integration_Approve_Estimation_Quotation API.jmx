    json = new JsonSlurper().parseText(response)
} catch (Exception e) {
    log.error(&quot;❌ Failed to parse JSON response: ${e.message}&quot;)
    vars.put(&quot;estimatorId&quot;, &quot;PARSE_ERROR&quot;)
    return
