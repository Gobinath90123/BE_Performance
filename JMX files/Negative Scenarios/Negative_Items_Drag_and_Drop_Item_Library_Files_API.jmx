def children = json?.data?.childProjectDetails
def childCount = 0

if (children &amp;&amp; children instanceof List) {
    children.eachWithIndex { child, i -&gt;
        def prefix = &quot;child_${i + 1}&quot; // Ex: child_1, child_2...

