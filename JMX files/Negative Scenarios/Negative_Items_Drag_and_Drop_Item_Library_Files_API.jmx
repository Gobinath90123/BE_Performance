logBlock &lt;&lt; &quot;\n================================================================\n&quot;
logBlock &lt;&lt; &quot;üèóÔ∏è  [JSR223] Extracting projectId for project title\n&quot;
logBlock &lt;&lt; &quot;üîç Matching PROJECT_TITLE : &apos;${projectTitle}&apos;\n&quot;
